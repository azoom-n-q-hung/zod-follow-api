{
  "name": "zod-follow-api",
  "version": "1.0.0-coreteam-2",
  "main": "dist/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azoom-n-q-hung/zod-follow-api.git"
  },
  "author": "Quoc Hung <n.q.hung@azoom.jp>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "setup": "cp .env.template .env",
    "dcc": "docker-compose -f ./cicd/development/docker-compose.yaml",
    "dcc:up": "yarn dcc up -d",
    "dcc:down": "yarn dcc down",
    "prisma:migrate": "yarn prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:generate": "yarn prisma generate --schema=./prisma/schema.prisma",
    "build": "yarn prisma:generate || true && tsc && tsc-alias",
    "lib:build": "yarn azoom-api-definition-util build",
    "lib:publish": "yarn azoom-api-definition-util publish",
    "dev": "concurrently \"tsc --watch\" \"tsc-alias --watch\" \"nodemon -q dist/index.js --exec 'node --es-module-specifier-resolution=node '\"",
    "start": "node --es-module-specifier-resolution=node dist/index.js"
  },
  "dependencies": {
    "@azoom/api-definition-util": "^0.1.9-alpha.0",
    "@azoom/nnn-router": "^1.0.0",
    "@azoom/node-util": "^1.1.1",
    "@prisma/client": "^3.11.1",
    "@zodios/core": "^10.7.7",
    "@zodios/express": "^10.5.1",
    "@zodios/openapi": "^10.4.7",
    "axios": "^1.3.4",
    "camelcase-keys": "^8.0.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.3",
    "express": "^4.17.3",
    "express-promise-router": "^4.1.1",
    "got": "^11.8.2",
    "lodash": "^4.17.21",
    "statuses": "^2.0.1",
    "swagger-ui-express": "^4.6.2",
    "zod": "3.21.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.6",
    "@types/statuses": "^2.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/wanakana": "^4.0.3",
    "concurrently": "^7.6.0",
    "glob": "^9.2.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "prisma": "^3.11.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.3",
    "typescript": "^4.9.5",
    "zod-prisma-types": "phongnd39/zod-prisma-types#79bbef06737c379946cb5240ab0be59182fa2d2c"
  },
  "engines": {
    "node": "16"
  },
  "azoom": {
    "api": {
      "publishName": "zod-follow",
      "outputFolder": "lib/zod-follow/dist",
      "schemaFolder": "lib/zod-follow/schemas",
      "additionalFiles": [
        "lib/zod-follow/schema.ts"
      ],
      "publishNamespace": "@azoom-n-q-hung"
    }
  }
}
